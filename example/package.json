{
  "name": "bpmn-js-bpmnlint-example",
  "version": "0.0.0",
  "description": "An example how to extend bpmn-js with model validation",
  "private": true,
  "scripts": {
    "dev": "webpack-serve --content ./public --open-path index.html",
    "pack-config": "bpmnlint-pack-config -c src/config.json -t es -o src/config-bundle.js",
    "bundle": "webpack --run-prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-bpmn-js-bpmnlint": "install-local ..",
    "postinstall": "run-s install-bpmn-js-bpmnlint pack-config"
  },
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "license": "MIT",
  "devDependencies": {
    "bpmn-js": "^2.5.1",
    "bpmn-js-bpmnlint": "^0.4.0",
    "bpmnlint-pack-config": "^0.3.0",
    "bpmnlint-plugin-camunda": "^0.2.0",
    "copy-webpack-plugin": "^4.5.2",
    "install-local": "^0.6.0",
    "npm-run-all": "^4.1.3",
    "raw-loader": "^0.5.1",
    "webpack": "^4.17.2",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.2"
  }
}
